 }).done(function(r){
 GLoading.carregar({'ativar':false});
 if(r.erro){Mensagens(r.mensagem, 3000, 'aviso');}
 else{
 Mensagens('Registro excluído com sucesso!', 1500, 'sucesso');
 if($(elemento).closest('.ggi-imagem').length){RemoverGGI($(elemento).closest('.ggi-imagem'));}
 setTimeout(function(){if($('.ggi-imagem').length && r.marcar_capa){$('.ggi-imagem').first().find('.tornar_capa').trigger('click');}},350);
