 if (retorno.aviso){$('#admin_centro_area-aviso').html(retorno.aviso.replace(/{moeda_cifrao}/g,"")).removeClass('esconder');}
 else{$('#admin_centro_area-aviso').addClass('esconder');}
 
 setTimeout(function(){
 var caixa = $('.gm').last().attr('id');
 if(caixa != undefined){ PosicionarGM(caixa);}
 bindEvents();
 },350);
 
