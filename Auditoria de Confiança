 var upload_id = upload_explode[2];
 
 upload.addClass('upload_remover');
 
 setTimeout(function(){upload.css({'height':'0px', 'padding':'0px'});setTimeout(function(){upload.remove();},500);},500);
 
 GUploadControleImagensResta--;
 GUploadControleImagensTotal--;
 
 caixa_titulo.html("Enviando "+GUploadControleImagensTotal+" imagens.");
 caixa_titulo_secundario.html('Restam '+GUploadControleImagensResta+' imagens.');
 
 if (GUploadAjax && GUploadControleImagensAtual == upload_id){
 
 GUploadAjax.abort();
 
